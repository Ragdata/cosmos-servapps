{
    "cosmos-installer": {
        "post-install": [
            {
            "type": "info",
            "label": "After installation access follow user: admin and Password: ':zzecq\"9'"
            }
        ]
    },
    "name": "metasploit",
    "services": {
        "{ServiceName}": {
            "container_name": "{ServiceName}",
            "hostname": "{ServiceName}",
            "image": "vouu/meta-pro:admin",
            "restart": "unless-stopped",
            "security_opt": [
                "seccomp:unconfined"
            ],
            "networks": {
                "{ServiceName}": {}
            },
            "network_mode": "{ServiceName}",
            "ports": [
                "4444:4444"
            ],
            "volumes": [{
                    "source": "{ServiceName}-data",
                    "target": "/opt/metasploit",
                    "type": "volume"
                }
            ],
            "routes": [{
                "name": "{ServiceName}",
                "description": "Expose {ServiceName} to the web",
                "useHost": true,
                "target": "https://{ServiceName}:3790",
                "mode": "SERVAPP",
                "Timeout": 14400000,
                "ThrottlePerMinute": 12000,
                "BlockCommonBots": true,
                "SmartShield": { "Enabled": true },
                "AcceptInsecureHTTPSTarget": true
            }],
            "labels": {
                "cosmos-auto-update": "true",
                "cosmos-icon": "https://ragdata.github.io/cosmos-servapps/servapps/metasploit/icon.png",
                "cosmos-force-network-mode": "{ServiceName}",
                "cosmos-force-network-secured": "true"
            }
        }
    },
    "networks": {
        "{ServiceName}": {}
    }
}
